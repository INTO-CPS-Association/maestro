{
  "fmus": {
    "{x1}": "watertankcontroller-c.fmu",
    "{x2}": "singlewatertank-20sim.fmu",
    "{x3}": "watertankcontroller-c.fmu"
  },
  "connections": {
    "{x1}.controller.valve": [
      "{x2}.tank.valvecontrol"
    ],
    "{x2}.tank.level": [
      "{x1}.controller.level"
    ]
  },
  "parameters": {
    "{x1}.controller.maxLevel": 2,
    "{x1}.controller.minLevel": 4
  },
  "algorithm": {
    "type": "fixed-step",
    "size": 0.1
  },
  "modelSwaps": {
    "controller": {
      "swapInstance": "crtlInstance3",
      "stepCondition": "(tank.level >0.2)",
      "swapCondition": "(tank.level >1)",
      "swapConnections": {
        "{x3}.crtlInstance3.valve": [
          "{x2}.wtInstance.valvecontrol"
        ],
        "{x2}.wtInstance.level": [
          "{x3}.crtlInstance3.level"
        ]
      }
    }
  },
  "modelTransfers": {
    "crtlInstance": "crtlInstance",
    "wtInstance": "wtInstance"
  }
}